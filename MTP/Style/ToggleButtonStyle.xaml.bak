<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp">

    <Style x:Key="toggleButton" TargetType="ToggleButton">
        <Setter Property="Height" Value="auto" />
        <Setter Property="Background" Value="#D6DAF3" />
        <Setter Property="Foreground" Value="Black" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Grid>
                        <Border
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Background="{TemplateBinding Background}"
                            BorderThickness="1"
                            CornerRadius="5">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />

                        </Border>
                        <!--<Rectangle Width="5"
                                  Height="30"
                                  Fill="{TemplateBinding BorderBrush}"
                                  Margin="3 0 0 0"
                                  HorizontalAlignment="Left"></Rectangle>-->
                        <Border
                            Width="6"
                            Height="auto"
                            Margin="3,12,0,12"
                            HorizontalAlignment="Left"
                            Background="{TemplateBinding BorderBrush}"
                            BorderThickness="0,0,0,0"
                            CornerRadius="5" />
                    </Grid>


                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--  Event Button  -->
        <Style.Triggers>
            <!--  Mouse Hover Back Color  -->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{StaticResource panelOverColor_LightSlateBlue}" />
                <Setter Property="Foreground" Value="{StaticResource titleColor_SteelBlue}" />
                <Setter Property="BorderBrush" Value="{Binding Path=Tag, RelativeSource={RelativeSource Self}}" />
            </Trigger>
            <!--  Active Button  -->
            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Margin" Value="0,0,0,0" />
                <Setter Property="Background" Value="#C8DFFA" />
                <Setter Property="Foreground" Value="{Binding Path=Tag, RelativeSource={RelativeSource Self}}" />
                <Setter Property="BorderBrush" Value="{Binding Path=Tag, RelativeSource={RelativeSource Self}}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <!--  Toggle Button Rectangle  -->
    <Style x:Key="ToggleButtonRectangle" TargetType="ToggleButton">
        <Setter Property="Height" Value="auto" />
        <Setter Property="Background" Value="#D6DAF3" />
        <Setter Property="Foreground" Value="Black" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect
                    Direction="315"
                    Opacity="0.5"
                    ShadowDepth="3"
                    Color="Gray" />
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Border
                        Margin="1"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="1"
                        CornerRadius="8">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontFamily="Montserrat"
                            FontSize="15"
                            FontWeight="Medium"
                            Foreground="{TemplateBinding Foreground}"
                            Text="{TemplateBinding Content}" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--  Event Button  -->
        <Style.Triggers>
            <!--  Mouse Hover Back Color  -->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{StaticResource panelOverColor_LightSlateBlue}" />
                <Setter Property="Foreground" Value="{StaticResource titleColor_SteelBlue}" />
            </Trigger>
            <!--  Active Button  -->
            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Background" Value="{Binding Path=Tag, RelativeSource={RelativeSource Self}}" />
                <Setter Property="Foreground" Value="White" />
                <Setter Property="FontWeight" Value="Bold" />
                <Setter Property="FontSize" Value="15" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <!--  Toggle Button Flip Flop Base  -->
    <Style x:Key="ToggleFlipFlopBase" TargetType="ToggleButton">
        <Setter Property="MaxHeight" Value="30" />
        <Setter Property="MaxWidth" Value="60" />
        <Setter Property="MinHeight" Value="30" />
        <Setter Property="MinWidth" Value="60" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Border
                        x:Name="border"
                        Background="Gray"
                        CornerRadius="15">
                        <Grid>
                            <Ellipse
                                x:Name="ellipse"
                                Width="22"
                                Margin="4"
                                HorizontalAlignment="Left"
                                Panel.ZIndex="1"
                                Fill="Blue" />
                            <ContentPresenter x:Name="PART_contentpresenter" />
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="ellipse" Property="HorizontalAlignment" Value="Right" />
                            <Setter TargetName="border" Property="Background" Value="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ToggleButton}, AncestorLevel=1}, Path=Tag}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  Toggle Button Click  -->
    <Style x:Key="ToggleClick" TargetType="ToggleButton">
        <Setter Property="MinHeight" Value="20" />
        <Setter Property="MinWidth" Value="20" />
        <Setter Property="Height" Value="80" />
        <Setter Property="Width" Value="80" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Border
                        x:Name="border"
                        Background="Transparent"
                        CornerRadius="60">
                        <Grid>
                            <Ellipse
                                x:Name="ellipseOn"
                                Width="20"
                                Height="20"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Fill="#FF9AFF80">
                                <Ellipse.BitmapEffect>
                                    <DropShadowBitmapEffect
                                        Direction="270"
                                        ShadowDepth="0.7"
                                        Softness="0.1"
                                        Color="#BBBBBB" />
                                </Ellipse.BitmapEffect>
                            </Ellipse>
                            <Ellipse
                                x:Name="ellipseOff"
                                Width="20"
                                Height="20"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Panel.ZIndex="1"
                                Fill="{StaticResource btncolor_LightCyan}">
                                <Ellipse.BitmapEffect>
                                    <DropShadowBitmapEffect
                                        Direction="270"
                                        ShadowDepth="0.7"
                                        Softness="0.1"
                                        Color="#BBBBBB" />
                                </Ellipse.BitmapEffect>
                            </Ellipse>
                            <Ellipse
                                x:Name="ellipseRipple"
                                Width="20"
                                Height="20"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Fill="#FF9AFF80" />
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True" />
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="ellipseOff" Property="Visibility" Value="Hidden" />
                            <Setter TargetName="ellipseOn" Property="Fill" Value="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ToggleButton}, AncestorLevel=1}, Path=Tag}" />
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <!--  G1 Animation  -->
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="(Ellipse.Width)"
                                            From="50"
                                            To="80"
                                            Duration="00:00:0.5" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="(Ellipse.Height)"
                                            From="50"
                                            To="80"
                                            Duration="00:00:0.5" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="Opacity"
                                            From="1.0"
                                            To="0.0"
                                            Duration="00:00:0.5" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <!--  G1 Animation  -->
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="(Ellipse.Width)"
                                            From="50"
                                            To="80"
                                            Duration="00:00:0.5" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="(Ellipse.Height)"
                                            From="50"
                                            To="80"
                                            Duration="00:00:0.5" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="Opacity"
                                            From="1.0"
                                            To="0.0"
                                            Duration="00:00:0.5" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>


                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  Toggle Button Flip Flop  -->
    <Style x:Key="ToggleFlipFlop" TargetType="{x:Type ToggleButton}">
        <Setter Property="Foreground" Value="Black" />
        <Setter Property="Background" Value="#FAFAFB" />
        <Setter Property="BorderBrush" Value="Gray" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />

        <Setter Property="Template">
            <Setter.Value>

                <ControlTemplate TargetType="ToggleButton">
                    <Border Width="60" Height="40">
                        <Grid>
                            <Border
                                x:Name="border"
                                Width="30"
                                Height="12"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Background="WhiteSmoke"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="0.5"
                                CornerRadius="3" />
                            <Ellipse
                                x:Name="ellipse"
                                Width="15"
                                Height="15"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Panel.ZIndex="2"
                                Fill="White"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="0.3">
                                <Ellipse.RenderTransform>
                                    <TranslateTransform X="-10" Y="0" />
                                </Ellipse.RenderTransform>
                                <Ellipse.BitmapEffect>
                                    <DropShadowBitmapEffect
                                        Direction="270"
                                        ShadowDepth="1"
                                        Softness="0.1"
                                        Color="#BBBBBB" />
                                </Ellipse.BitmapEffect>
                            </Ellipse>
                            <Ellipse
                                x:Name="ellipseRipple"
                                Width="15"
                                Height="15"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Panel.ZIndex="1"
                                Fill="{TemplateBinding Background}">
                                <Ellipse.RenderTransform>
                                    <TranslateTransform X="-10" Y="0" />
                                </Ellipse.RenderTransform>
                            </Ellipse>
                            <TextBlock Text="{TemplateBinding Content}">
                                <TextBlock.RenderTransform>
                                    <TranslateTransform X="-20" Y="10" />
                                </TextBlock.RenderTransform>
                            </TextBlock>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="ellipse" Property="Fill" Value="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ToggleButton}, AncestorLevel=1}, Path=Background}" />
                            <Setter TargetName="border" Property="Background" Value="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ToggleButton}, AncestorLevel=1}, Path=Tag}" />
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>


                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ellipse" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.X)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="0" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.6, 1"
                                                KeyTime="0:0:1.4"
                                                Value="10" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ellipseRipple" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.X)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="-10" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.6, 1"
                                                KeyTime="0:0:1.4"
                                                Value="12" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ellipseRipple" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.Y)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="0" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.6, 1"
                                                KeyTime="0:0:1.4"
                                                Value="0" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="(Ellipse.Width)"
                                            From="15"
                                            To="30"
                                            Duration="00:00:0.5" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="(Ellipse.Height)"
                                            From="15"
                                            To="30"
                                            Duration="00:00:0.5" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="Opacity"
                                            From="1.0"
                                            To="0.0"
                                            Duration="00:00:0.5" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ellipse" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.X)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="15" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="-12" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ellipseRipple" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.X)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="5" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="-10" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ellipseRipple" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.Y)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="0" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="0" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="(Ellipse.Width)"
                                            From="15"
                                            To="30"
                                            Duration="00:00:0.5" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="(Ellipse.Height)"
                                            From="15"
                                            To="30"
                                            Duration="00:00:0.5" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="ellipseRipple"
                                            Storyboard.TargetProperty="Opacity"
                                            From="1.0"
                                            To="0.0"
                                            Duration="00:00:0.5" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  Toggle Button Slide  -->
    <Style x:Key="ToggleSlide" TargetType="ToggleButton">
        <Setter Property="MaxHeight" Value="50" />
        <Setter Property="MaxWidth" Value="120" />
        <Setter Property="MinHeight" Value="10" />
        <Setter Property="MinWidth" Value="30" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Border
                        x:Name="BorderOut"
                        Background="{StaticResource color_WhiteSmoke}"
                        BorderThickness="0"
                        CornerRadius="5">
                        <Grid>

                            <Border
                                x:Name="BorderClosed"
                                Width="57"
                                Height="45"
                                Margin="2"
                                HorizontalAlignment="Left"
                                Background="{StaticResource btncolor_LightCoral}"
                                BorderBrush="Black"
                                BorderThickness="0.2"
                                CornerRadius="5">
                                <TextBlock
                                    x:Name="txtStateClose"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Text="Closed" />
                                <Border.BitmapEffect>
                                    <DropShadowBitmapEffect
                                        Direction="270"
                                        ShadowDepth="2"
                                        Softness="0.1"
                                        Color="#BBBBBB" />
                                </Border.BitmapEffect>
                                <Border.RenderTransform>
                                    <TranslateTransform X="0" Y="0" />
                                </Border.RenderTransform>
                            </Border>
                            <Border
                                x:Name="BorderOpen"
                                Width="53"
                                Height="40"
                                Margin="2"
                                HorizontalAlignment="Right"
                                Background="{StaticResource btncolor_LightCyan}"
                                BorderBrush="Black"
                                BorderThickness="0.2"
                                CornerRadius="5"
                                Visibility="Visible">
                                <TextBlock
                                    x:Name="txtStateOpen"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Text="Open" />
                                <Border.BitmapEffect>
                                    <DropShadowBitmapEffect
                                        Direction="270"
                                        ShadowDepth="0"
                                        Softness="0.1"
                                        Color="#BBBBBB" />
                                </Border.BitmapEffect>
                                <Border.RenderTransform>
                                    <TranslateTransform X="-2" Y="0" />
                                </Border.RenderTransform>
                            </Border>

                        </Grid>
                        <Border.BitmapEffect>
                            <DropShadowBitmapEffect
                                Direction="270"
                                ShadowDepth="1.7"
                                Softness="0.1"
                                Color="#BBBBBB" />
                        </Border.BitmapEffect>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="txtStateClose" Property="Text" Value="Close" />
                            <Setter TargetName="txtStateOpen" Property="Text" Value="Opened" />
                            <Setter TargetName="BorderClosed" Property="Background" Value="{StaticResource btncolor_LightCyan}" />
                            <Setter TargetName="BorderOpen" Property="Background" Value="{StaticResource color_MediumSpringGreen}" />

                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <!--  closed  -->
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BorderClosed" Storyboard.TargetProperty="(Border.BitmapEffect).(DropShadowBitmapEffect.ShadowDepth)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="2" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="0" />
                                        </DoubleAnimationUsingKeyFrames>

                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BorderClosed" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.X)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="0" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="2" />
                                        </DoubleAnimationUsingKeyFrames>

                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderClosed"
                                            Storyboard.TargetProperty="(Border.Width)"
                                            From="57"
                                            To="53"
                                            Duration="00:00:0.25" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderClosed"
                                            Storyboard.TargetProperty="(Border.Height)"
                                            From="45"
                                            To="40"
                                            Duration="00:00:0.25" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderClosed"
                                            Storyboard.TargetProperty="Opacity"
                                            From="1.0"
                                            To="1.0"
                                            Duration="00:00:0.5" />

                                        <!--  Open  -->
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BorderOpen" Storyboard.TargetProperty="(Border.BitmapEffect).(DropShadowBitmapEffect.ShadowDepth)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="0" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="2" />
                                        </DoubleAnimationUsingKeyFrames>

                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BorderOpen" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.X)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="-2" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="0" />
                                        </DoubleAnimationUsingKeyFrames>

                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderOpen"
                                            Storyboard.TargetProperty="(Border.Width)"
                                            From="53"
                                            To="57"
                                            Duration="00:00:0.25" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderOpen"
                                            Storyboard.TargetProperty="(Border.Height)"
                                            From="40"
                                            To="45"
                                            Duration="00:00:0.25" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderOpen"
                                            Storyboard.TargetProperty="Opacity"
                                            From="1.0"
                                            To="1.0"
                                            Duration="00:00:0.5" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <!--  Open  -->
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BorderOpen" Storyboard.TargetProperty="(Border.BitmapEffect).(DropShadowBitmapEffect.ShadowDepth)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="2" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="0" />
                                        </DoubleAnimationUsingKeyFrames>

                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BorderOpen" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.X)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="0" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="-2" />
                                        </DoubleAnimationUsingKeyFrames>

                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderOpen"
                                            Storyboard.TargetProperty="(Border.Width)"
                                            From="57"
                                            To="53"
                                            Duration="00:00:0.25" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderOpen"
                                            Storyboard.TargetProperty="(Border.Height)"
                                            From="45"
                                            To="40"
                                            Duration="00:00:0.25" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderOpen"
                                            Storyboard.TargetProperty="Opacity"
                                            From="1.0"
                                            To="1.0"
                                            Duration="00:00:0.25" />

                                        <!--  Closed  -->

                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BorderClosed" Storyboard.TargetProperty="(Border.BitmapEffect).(DropShadowBitmapEffect.ShadowDepth)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="0" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="2" />
                                        </DoubleAnimationUsingKeyFrames>

                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BorderClosed" Storyboard.TargetProperty="(Ellipse.RenderTransform).(TranslateTransform.X)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="2" />
                                            <SplineDoubleKeyFrame
                                                KeySpline="0, 1, 0.5, 1"
                                                KeyTime="0:0:1.4"
                                                Value="0" />
                                        </DoubleAnimationUsingKeyFrames>

                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderClosed"
                                            Storyboard.TargetProperty="(Border.Width)"
                                            From="53"
                                            To="57"
                                            Duration="00:00:0.25" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderClosed"
                                            Storyboard.TargetProperty="(Border.Height)"
                                            From="40"
                                            To="45"
                                            Duration="00:00:0.25" />
                                        <DoubleAnimation
                                            BeginTime="00:00:00"
                                            Storyboard.TargetName="BorderClosed"
                                            Storyboard.TargetProperty="Opacity"
                                            From="1.0"
                                            To="1.0"
                                            Duration="00:00:0.5" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  Toggle Button Buzzer  -->
    <Style x:Key="ToggleBuzzer" TargetType="ToggleButton">
        <Setter Property="MaxHeight" Value="40" />
        <Setter Property="MaxWidth" Value="90" />
        <Setter Property="MinHeight" Value="0" />
        <Setter Property="MinWidth" Value="0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <StackPanel Orientation="Horizontal">
                        <Grid>
                            <fa:IconImage
                                Name="normal"
                                Icon="Bullhorn"
                                Stretch="Fill" />
                            <fa:IconImage
                                Name="pressed"
                                Foreground="Red"
                                Icon="X"
                                Stretch="Uniform"
                                Visibility="Hidden" />
                        </Grid>
                        <TextBlock VerticalAlignment="Center"><Bold>Buzzer</Bold></TextBlock>
                    </StackPanel>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="normal" Property="Visibility" Value="Visible" />
                            <Setter TargetName="pressed" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsChecked" Value="False">
                            <Setter TargetName="normal" Property="Visibility" Value="Visible" />
                            <Setter TargetName="pressed" Property="Visibility" Value="Hidden" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>