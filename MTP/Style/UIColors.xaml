    <SolidColorBrush x:Key="btncolor_LightCoral" Color="#F38181" />
    <SolidColorBrush x:Key="btncolor_DarkTurquoise" Color="#08D9D6" />
    <SolidColorBrush x:Key="btncolor_YourPink" Color="#FFC7C7" />
