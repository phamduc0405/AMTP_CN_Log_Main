<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp"
    xmlns:local="clr-namespace:ACO2.Style">
    <!--  TextBox Standard  -->
    <Style x:Key="TextBoxStandard" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="White" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Grid>
                        <Rectangle
                            x:Name="rtOut"
                            Fill="#FFA4BCC6"
                            RadiusX="4"
                            RadiusY="4"
                            Visibility="Hidden" />
                        <Border
                            x:Name="bdr"
                            Width="{TemplateBinding Width}"
                            Height="{TemplateBinding Height}"
                            Background="White"
                            BorderBrush="{StaticResource gradientLayout5}"
                            BorderThickness="1"
                            CornerRadius="3">

                            <Grid>
                                <TextBox
                                    x:Name="SearchBox"
                                    Margin="1,0,0,0"
                                    VerticalAlignment="Center"
                                    Background="White"
                                    BorderThickness="0"
                                    FontFamily="Montserrat"
                                    FontSize="{TemplateBinding FontSize}"
                                    FontWeight="Medium"
                                    Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                <TextBlock
                                    Grid.Column="1"
                                    Margin="10,5,5,5"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontSize="{TemplateBinding FontSize}"
                                    Foreground="DarkGray"
                                    IsHitTestVisible="False"
                                    Text="{TemplateBinding Tag}">
                                    <TextBlock.Style>
                                        <Style TargetType="{x:Type TextBlock}">
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding Text, ElementName=SearchBox}" Value="">
                                                    <Setter Property="Visibility" Value="Visible" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                            <Setter Property="Visibility" Value="Hidden" />
                                        </Style>
                                    </TextBlock.Style>
                                </TextBlock>
                            </Grid>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="rtOut" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="rtOut" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="rtOut" Property="Visibility" Value="Visible" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  Modern TextBox  -->
    <Style x:Key="ModernTextBox" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="#353340" />
        <Setter Property="Foreground" Value="#CFCFCF" />
        <Setter Property="Tag" Value="SearchBox" />

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border
                        Width="{TemplateBinding Width}"
                        Height="{TemplateBinding Height}"
                        Background="{TemplateBinding Background}"
                        CornerRadius="10">
                        <Grid>
                            <!--  Nội dung TextBox  -->
                            <Rectangle StrokeThickness="0" />
                            <TextBox
                                x:Name="SearchBox"
                                Margin="1"
                                Padding="5"
                                VerticalAlignment="Center"
                                Background="Transparent"
                                BorderThickness="0"
                                FontFamily="Montserrat"
                                FontSize="{TemplateBinding FontSize}"
                                FontWeight="Medium"
                                Foreground="{TemplateBinding Foreground}"
                                Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <!--  Placeholder  -->
                            <TextBlock
                                x:Name="PlaceholderText"
                                Margin="10,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontSize="15"
                                Foreground="DarkGray"
                                IsHitTestVisible="False"
                                Text="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}">

                                <TextBlock.Style>
                                    <Style TargetType="{x:Type TextBlock}">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Text, ElementName=SearchBox}" Value="">
                                                <Setter Property="Visibility" Value="Visible" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                        <Setter Property="Visibility" Value="Hidden" />
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>

                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  TextBoxStrech  -->
    <Style x:Key="TextboxStrech" TargetType="{x:Type TextBox}">
        <Setter Property="Text" Value="000000" />
        <Setter Property="FontFamily" Value="Montserrat" />
        <Setter Property="FontWeight" Value="Medium" />

        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="FontSize" Value="15" />
    </Style>
    <!--  Floating Hint TextBox Style No Border  -->
    <Style x:Key="TextBoxFloatingHintStyleNoBorder" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border
                        Margin="5"
                        BorderBrush="#353340"
                        BorderThickness="0"
                        CornerRadius="5">
                        <Grid>
                            <!--  Floating Hint  -->
                            <TextBlock
                                x:Name="FloatingHint"
                                Margin="5,3,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                FontFamily="Montserrat"
                                FontSize="15"
                                FontWeight="Medium"
                                Foreground="Gray"
                                IsHitTestVisible="False"
                                Text="{TemplateBinding Tag}">
                                <TextBlock.RenderTransform>
                                    <TranslateTransform X="0" Y="15" />
                                </TextBlock.RenderTransform>
                            </TextBlock>

                            <!--  Main TextBox  -->
                            <ScrollViewer Margin="5,20,5,5" VerticalScrollBarVisibility="Hidden">
                                <TextBox
                                    x:Name="SearchBox"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    FontFamily="Montserrat"
                                    FontSize="15"
                                    FontWeight="Medium"
                                    Foreground="Black"
                                    Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                            </ScrollViewer>

                            <!--  Underline  -->
                            <Rectangle
                                x:Name="Underline"
                                Height="1.5"
                                Margin="5,0,5,5"
                                VerticalAlignment="Bottom"
                                Fill="Green"
                                StrokeThickness="2" />
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <!--  Trigger for Text Input  -->
                        <Trigger Property="Text" Value="">
                            <Setter TargetName="FloatingHint" Property="FontSize" Value="15" />
                            <Setter TargetName="FloatingHint" Property="Foreground" Value="Gray" />
                            <Setter TargetName="FloatingHint" Property="FontWeight" Value="Medium" />
                            <Setter TargetName="FloatingHint" Property="FontFamily" Value="Montserrat" />
                            <Setter TargetName="FloatingHint" Property="RenderTransform">
                                <Setter.Value>
                                    <TranslateTransform X="0" Y="15" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="Underline" Property="Fill" Value="Gray" />
                        </Trigger>
                        <Trigger Property="IsKeyboardFocusWithin" Value="True">
                            <Setter TargetName="FloatingHint" Property="FontSize" Value="12" />
                            <Setter TargetName="FloatingHint" Property="RenderTransform">
                                <Setter.Value>
                                    <TranslateTransform X="0" Y="-10" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="Underline" Property="Fill" Value="#3F51B5" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--  TextBox Hint Style No Border  -->
    <Style x:Key="TextBoxHintStyleNoBorder" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border
                        Width="{TemplateBinding Width}"
                        Height="{TemplateBinding Height}"
                        Background="{TemplateBinding Background}"
                        CornerRadius="10">
                        <Grid>
                            <!--  Nội dung chính của TextBox  -->
                            <TextBox
                                x:Name="MainTextBox"
                                Margin="10,0,10,0"
                                Padding="5"
                                VerticalAlignment="Center"
                                Background="Transparent"
                                BorderThickness="0"
                                FontFamily="Montserrat"
                                FontSize="15"
                                FontWeight="Medium"
                                Foreground="{TemplateBinding Foreground}"
                                Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <!--  Placeholder hiển thị nền khi TextBox rỗng  -->
                            <TextBlock
                                x:Name="Placeholder"
                                Margin="15,0,10,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontFamily="Montserrat"
                                FontSize="15"
                                FontWeight="Medium"
                                Foreground="DarkGray"
                                IsHitTestVisible="False"
                                Text="{TemplateBinding Tag}">
                                <TextBlock.Style>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="Visibility" Value="Hidden" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Text, ElementName=MainTextBox}" Value="">
                                                <Setter Property="Visibility" Value="Visible" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  TextBox Password Style No Border  -->
    <Style x:Key="TextBoxPasswordStyleNoBorder" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border
                        Width="{TemplateBinding Width}"
                        Height="{TemplateBinding Height}"
                        Background="{TemplateBinding Background}"
                        CornerRadius="10">
                        <Grid>
                            <!--  Nội dung chính của TextBox  -->
                            <TextBox
                                x:Name="MainTextBox"
                                Margin="10,0,10,0"
                                Padding="5"
                                VerticalAlignment="Center"
                                Background="Transparent"
                                BorderThickness="0"
                                Foreground="{TemplateBinding Foreground}"
                                Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <!--  Placeholder hiển thị nền khi TextBox rỗng  -->
                            <TextBlock
                                x:Name="Placeholder"
                                Margin="10,0,10,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontSize="15"
                                Foreground="DarkGray"
                                IsHitTestVisible="False"
                                Text="{TemplateBinding Tag}">
                                <TextBlock.Style>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="Visibility" Value="Hidden" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Text, ElementName=MainTextBox}" Value="">
                                                <Setter Property="Visibility" Value="Visible" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <DataTrigger Binding="{Binding Visibility, ElementName=passbox, Mode=TwoWay}" Value="Visible">
                <Setter Property="Visibility" Value="Hidden" />
            </DataTrigger>
        </Style.Triggers>
    </Style>
    <!--  TextBox Limit Character  -->
    <Style x:Key="TextBoxLimitCharacterStyle" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border
                        Margin="5"
                        BorderBrush="#353340"
                        BorderThickness="0"
                        CornerRadius="5">
                        <Grid>
                            <!--  Main TextBox  -->
                            <ScrollViewer Margin="5,20,5,5" VerticalScrollBarVisibility="Hidden">
                                <TextBox
                                    x:Name="MainTextBox"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    FontFamily="Montserrat"
                                    FontSize="15"
                                    FontWeight="Medium"
                                    Foreground="Black" />
                            </ScrollViewer>

                            <!--  Underline  -->
                            <Rectangle
                                x:Name="Underline"
                                Height="1.5"
                                Margin="5,0,5,5"
                                VerticalAlignment="Bottom"
                                Fill="Gray"
                                StrokeThickness="2" />

                            <!--  Character Counter  -->
                            <TextBlock
                                x:Name="CharacterCounter"
                                Margin="0,0,10,-10"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Bottom"
                                FontFamily="Montserrat"
                                FontSize="15"
                                Foreground="Gray" />
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <!--  Trigger for Focus  -->
                        <Trigger Property="IsKeyboardFocusWithin" Value="True">
                            <Setter TargetName="Underline" Property="Fill" Value="#3F51B5" />
                        </Trigger>
                        <!--  Trigger for Losing Focus  -->
                        <Trigger Property="IsKeyboardFocusWithin" Value="False">
                            <Setter TargetName="Underline" Property="Fill" Value="Gray" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  TextBox Multi Line  -->
    <Style x:Key="TextBoxMultiLine" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="White" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Grid>
                        <Rectangle
                            x:Name="rtOut"
                            Fill="#FFA4BCC6"
                            RadiusX="4"
                            RadiusY="4"
                            Visibility="Hidden" />
                        <Border
                            x:Name="bdr"
                            Width="{TemplateBinding Width}"
                            Height="{TemplateBinding Height}"
                            Background="White"
                            BorderBrush="{StaticResource gradientLayout5}"
                            BorderThickness="1"
                            CornerRadius="3">
                            <Grid>
                                <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto">
                                    <TextBox
                                        x:Name="SearchBox"
                                        Margin="10,5,5,5"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        AcceptsReturn="True"
                                        AcceptsTab="True"
                                        Background="White"
                                        BorderThickness="0"
                                        FontFamily="Montserrat"
                                        FontSize="15"
                                        FontWeight="Medium"
                                        Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        TextWrapping="Wrap"
                                        VerticalScrollBarVisibility="Auto" />
                                </ScrollViewer>

                                <TextBlock
                                    Grid.Column="1"
                                    Margin="10,5,5,5"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    FontSize="15"
                                    Foreground="DarkGray"
                                    IsHitTestVisible="False"
                                    Text="{TemplateBinding Tag}">
                                    <TextBlock.Style>
                                        <Style TargetType="{x:Type TextBlock}">
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding Text, ElementName=SearchBox}" Value="">
                                                    <Setter Property="Visibility" Value="Visible" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                            <Setter Property="Visibility" Value="Hidden" />
                                        </Style>
                                    </TextBlock.Style>
                                </TextBlock>
                            </Grid>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="rtOut" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="rtOut" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="rtOut" Property="Visibility" Value="Visible" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  Password Box  -->
    <Style x:Key="PasswordBoxStyle" TargetType="{x:Type PasswordBox}">

        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FontFamily" Value="Montserrat" />
        <Setter Property="FontSize" Value="15" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="0" />
        <Style.Triggers>
            <DataTrigger Binding="{Binding FontSize}" Value="15">
                <DataTrigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard SpeedRatio="10">
                            <DoubleAnimation Storyboard.TargetProperty="FontSize" To="20" />
                            <ThicknessAnimation Storyboard.TargetProperty=" Margin" To="0 20 0 0" />
                        </Storyboard>
                    </BeginStoryboard>
                </DataTrigger.EnterActions>

                <DataTrigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard SpeedRatio="10">
                            <DoubleAnimation Storyboard.TargetProperty="FontSize" To="15" />
                            <ThicknessAnimation Storyboard.TargetProperty="Margin" To="0" />
                        </Storyboard>
                    </BeginStoryboard>
                </DataTrigger.ExitActions>
            </DataTrigger>

            <DataTrigger Binding="{Binding IsPressed, ElementName=ShowPass}" Value="True">
                <Setter Property="Visibility" Value="Hidden" />
            </DataTrigger>

        </Style.Triggers>
    </Style>
    <!--  TextBox IP Address  -->
    <Style x:Key="IpAddressTextBoxStyle" TargetType="{x:Type TextBox}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Grid>
                        <Rectangle
                            Fill="#FFA4BCC6"
                            RadiusX="4"
                            RadiusY="4"
                            Visibility="Visible" />
                        <Border
                            Background="White"
                            BorderBrush="{StaticResource gradientLayout5}"
                            BorderThickness="1"
                            CornerRadius="3">
                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <TextBox
                                    x:Name="Octet1"
                                    Grid.Column="0"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    FontFamily="Montserrat"
                                    FontSize="15"
                                    FontWeight="Medium"
                                    MaxLength="3"
                                    TextAlignment="Center" />
                                <TextBlock
                                    Grid.Column="1"
                                    VerticalAlignment="Center"
                                    FontSize="15"
                                    Foreground="Black"
                                    Text="." />
                                <TextBox
                                    x:Name="Octet2"
                                    Grid.Column="2"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    FontFamily="Montserrat"
                                    FontSize="15"
                                    FontWeight="Medium"
                                    MaxLength="3"
                                    TextAlignment="Center" />
                                <TextBlock
                                    Grid.Column="3"
                                    VerticalAlignment="Center"
                                    FontSize="15"
                                    Foreground="Black"
                                    Text="." />
                                <TextBox
                                    x:Name="Octet3"
                                    Grid.Column="4"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    FontFamily="Montserrat"
                                    FontSize="15"
                                    FontWeight="Medium"
                                    MaxLength="3"
                                    TextAlignment="Center" />
                                <TextBlock
                                    Grid.Column="5"
                                    VerticalAlignment="Center"
                                    FontSize="15"
                                    Foreground="Black"
                                    Text="." />
                                <TextBox
                                    x:Name="Octet4"
                                    Grid.Column="6"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    FontFamily="Montserrat"
                                    FontSize="15"
                                    FontWeight="Medium"
                                    MaxLength="3"
                                    TextAlignment="Center" />
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  RichTextBox  -->
    <Style x:Key="RichTextBoxStyle" TargetType="{x:Type RichTextBox}">
        <Setter Property="Background" Value="White" />
        <Setter Property="BorderBrush" Value="Gray" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="10" />
        <Setter Property="VerticalScrollBarVisibility" Value="Auto" />
        <Setter Property="FontFamily" Value="Montserrat" />
        <Setter Property="FontSize" Value="15" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="AcceptsReturn" Value="True" />
        <Setter Property="AcceptsTab" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type RichTextBox}">
                    <Border
                        Padding="{TemplateBinding Padding}"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="5">
                        <AdornerDecorator>
                            <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="{TemplateBinding VerticalScrollBarVisibility}">
                                <RichTextBox
                                    Background="Transparent"
                                    BorderThickness="0"
                                    FontFamily="{TemplateBinding FontFamily}"
                                    FontSize="{TemplateBinding FontSize}"
                                    FontWeight="{TemplateBinding FontWeight}"
                                    IsReadOnly="{TemplateBinding IsReadOnly}" />
                            </ScrollViewer>
                        </AdornerDecorator>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>
